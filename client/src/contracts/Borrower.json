  "source": "pragma solidity ^0.5.16;\n\nimport \"./WrappedETCToken.sol\";\nimport \"@openzeppelin/contracts/ownership/Ownable.sol\";\n\ncontract Borrower is Ownable {\n    WrappedETCToken wETC = WrappedETCToken(address(0x0)); // address of on chain wrappedetc token\n\n    function initFlashLoan(uint256 amount) public onlyOwner {\n        wETC.flash(amount);\n    }\n\n    function doFlashLoan() public {\n        // do whatever\n    }\n}",
